{
  "manifest_version": 3,
  "name": "TALP",
  "version": "1.0",
  "description": "Save and manage your favorite tabs.",
  "permissions":["tabs" , "activetabs" , "storage", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}